{
	"name": "potree-core",
	"version": "2.0.0",
	"description": "Potree wrapper for three.js applications",
	"repository": {
		"type": "git",
		"url": "https://github.com/tentone/potree-core.git"
	},
	"files": ["dist"],
	"main": "./dist/potree-loader.umd.js",
	"module": "./dist/potree-loader.es.js",
	"types": "./dist/potree-loader.d.ts",
	"type": "module",
	"scripts": {
		"start": "webpack --mode development --watch --progress --stats-children",
		"build": "webpack --mode production --config webpack.prod.js",
		"example": "npm run build --stats-children && webpack-dev-server --config webpack.example.js --mode development --progress --port 5200",
		"docs": "jsdoc -d docs source",
		"pub": "npm run build && npm publish --access public .",
		"lint": "eslint --ext .js source",
		"lint-fix": "eslint --ext .js --fix source"
	},
	"keywords": [
		"three",
		"potree",
		"3d",
		"webgl"
	],
	"author": "Tentone",
	"license": "MIT",
	"peerDependencies": {
		"three": "^0.120.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.59",
		"@types/three": "^0.148.0",
		"css-loader": "^5.2.7",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.2",
		"prettier": "1.19.1",
		"raw-loader": "^4.0.2",
		"rimraf": "^3.0.2",
		"style-loader": "^2.0.0",
		"three": "^0.148.0",
		"ts-loader": "^9.4.0",
		"tslint": "^6.1.3",
		"typescript": "^4.8.3",
		"@typescript-eslint/eslint-plugin": "5.47.0",
		"@typescript-eslint/parser": "5.47.0",
		"eslint": "8.30.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-tsdoc": "0.2.17",
		"eslint-plugin-typescript": "0.14.0",
		"@types/three": "^0.148.0",
		"concurrently": "7.6.0",
		"path": "0.12.7",
		"three": "^0.148.0",
		"typescript": "4.9.4",
		"webpack": "^5.74.0",
		"webpack-bundle-analyzer": "^4.6.1",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1",
		"worker-loader": "^3.0.8"
	}
}
